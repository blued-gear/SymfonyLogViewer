@config "./tailwind.config.js";
@import "tailwindcss";
@custom-variant dark (&:where(.dark, .dark *));
@source inline("bg-neutral-500 text-white font-bold inline-block rounded-full");

.filterSelect {
    width: 52rem;
}

.filterSelect select {
    width: 100%;
}

.boxedBtn {
    border: solid 1px;
    background-color: lightgray;
    padding: 0.2rem;
    cursor: pointer
}
